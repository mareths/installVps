
#Mise en couleur des messages
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
 
#Cr√©ation d'alias
[alias]
        co = checkout
        st = status
        ci = commit